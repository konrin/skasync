{
    "RootDir": "../thesoul",
    "Pods": [
        {
            "Artifact": "thesoul/thesoul-dev",
            "Selector": "app=php-nginx",
            "Container": "php",
            "RootDir": "/app",
            "DockerfileDir": "../thesoul/.tsp/dev/docker/fpm"
        },
        {
            "Artifact": "thesoul/thesoul-workers-dev",
            "Selector": "app=php-supervisor",
            "Container": "php",
            "RootDir": "/app",
            "DockerfileDir": "../thesoul/.tsp/dev/docker/workers"
        }
    ],
    "Skaffold": {
        "Addr": "localhost:50052"
    },
    "Sync": {
        "AfterDeployOrStart": null,
        "Debounce": 1000
    }
}
